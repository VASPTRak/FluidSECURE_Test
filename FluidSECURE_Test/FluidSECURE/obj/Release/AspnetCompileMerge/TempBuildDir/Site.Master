		<script>
			function CheckSession() {
				var FromLoginPage = '<%= Session("FromLoginPage") %>';
				if (FromLoginPage == "Y") {
					window.location.href = "/Account/Login"
				}
				else {
					$('#checkLoginBox').modal({
						show: true,
						backdrop: 'static',
						keyboard: false
					});
				}
			}

			function NotValidUser() {
				$('#NotValidUser').modal({
					show: true,
					backdrop: 'static',
					keyboard: false
				});
			}

		</script>
